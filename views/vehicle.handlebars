
<!doctype html>
<html>
<head>
    
    <link rel="stylesheet" type="text/css" href="css/signinup.css">   
    <title>Enter vehicle info </title>
</head>
    
    <body>
    
    <div class="login-box" style="width:300px; height:900">
    <img src="avatar.png" class="avatar">

        <h1>Welcome!!</h1>
            {{#if error}}
            
            <p> <em>{{error}}</em> </p>
            {{/if}}
            <form method="POST">
         
 <input type="text" required name="type" placeholder="Enter fuel">
            <input type="text" required name="origin" placeholder="Enter src">
<input type="text" name="destination" placeholder="Enter dest">
<input type="text" required name="mileage" placeholder="Enter mileage">
<input type="text" required name="mileage_unit" placeholder="Enter mileage unit">

            <input id="#signin" type="submit" name="submit" value="Login">
            <a href="/signup" id="#signup">Don't have an Account?</a>   <br>
            </form>
        
    </div>
        <script src="./javascript/frontsignin.js"></script>
    </body>
</html>



